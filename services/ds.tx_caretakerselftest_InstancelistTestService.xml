<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
	<meta>
		<langDisable>1</langDisable>
	</meta>
	<ROOT>
		<TCEforms> 
			<sheetTitle>HTTP-Communication</sheetTitle>
		</TCEforms>
		<type>array</type>
		<el>	
			<mode>
				<TCEforms>
					<label>Select the desired result</label>
					<config>
						<type>select</type>
					    <items>
					    	<numIndex index="0">
					    		<numIndex index="0">List Url</numIndex>
								<numIndex index="1">url</numIndex>
					    	</numIndex>
					    	<numIndex index="1">
					    		<numIndex index="0">Plain List</numIndex>
								<numIndex index="1">plain</numIndex>
					    	</numIndex>
					  	</items>
					  	<default>plain</default>
					</config>
				</TCEforms>
			</mode>
			<instancelist_url>
				<TCEforms>
				    <displayCond>FIELD:mode:=:url</displayCond>
					<label>URL to fetch the list of Instance URLs from</label>
					<config>
						<type>input</type>
					</config>
				</TCEforms>
			</instancelist_url>
			<instancelist_plain>
				<TCEforms>
					<displayCond>FIELD:mode:=:plain</displayCond>
					<label>Select the desired result</label>
					<config>
						<type>text</type>
					</config>
				</TCEforms>
			</instancelist_plain>
		</el>
	</ROOT>
</T3DataStructure>